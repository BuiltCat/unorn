{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2016",
    "module": "ESNext",
    "lib": ["ESNext"],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@unocss-native/preset-react-native": ["./packages/preset-react-native/src/index.ts"],
      "@unocss-native/core": ["./packages/core/src/index.ts"],
      "@unocss-native/transformer": ["./packages/transformer/src/index.ts"]
    }
  }
}
